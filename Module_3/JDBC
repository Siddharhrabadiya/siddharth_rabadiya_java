CREATE OR REPLACE TRIGGER  salarytrigger
AFTER INSERT OR UPDATE OR DELETE ON EmpTable;
 FOR EACH ROW
 DECLARE
 
 BEGIN
 UPDATE THEN
 ACTION ='update';
 INSERT INTO EmpLogTable VALUES(USER,:NEW.New_salary);
 END;
