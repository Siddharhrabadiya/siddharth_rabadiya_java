14.
select Ename from EmpTable where Deptno=20 and Job in ( select distinct Job from EmpTable where Deptno=10)

15.
Select distinct * from EmpTable where Sal in(select Sal from EmpTable where Ename in ('FORD','SMITH'))
and Ename not in ('FORD','SMITH') Order By Sal Desc ;

16.
SELECT * FROM emp WHERE Job IN(SELECT Job FROM EmpTable WHERE Ename IN('SMITH','ALLEN'))
 AND Ename NOT IN('SMITH','ALLEN')

17.
SELECT * FROM EmpTable WHERE Deptno=20 AND Job IN(SELECT Job FROM emp WHERE Deptno=10)

18.
